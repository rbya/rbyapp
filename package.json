{
    "name": "rbyapp",
    "version": "1.0.0",
    "description": "RBYA Mobile app",
    "main": "index.js",
    "scripts": {
        "test:client": "",
        "test:service": "cd service && yarn run test",
        "init:client": "cd client && yarn install",
        "init:service": "cd service && yarn install",
        "start:client": "cd client && yarn run ionic:serve",
